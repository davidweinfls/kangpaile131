function : void main()
{
	float a = 5;
	float b = 6;
	float c = 7;
    
	a = ++b + c; // a = 14, b = 7
	//cout<<a<<endl;
	b = ++a + ++c; // a = 15, c = 8, b = 23
	c = ++a - --b; // c = -6, a = 16, b = 22
	
	cout << "a is: " << a << endl; // 16
	cout << "b is: " << b << endl; // 22
	cout << "c is: " << c << endl; // -6
}